    "circular-json": "^0.5.0",
    "error-stack-parser": "^2.0.1",
    "rxjs-report-usage": "^1.0.4",
    "stacktrace-gps": "^3.0.2"
  },
  "description": "A debugging library for RxJS",
  "devDependencies": {
    "@babel/core": "^7.11.1",
